<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Page Object Model">

	<listeners>

		<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
		<listener class-name="com.google.listeners.CustomListeners"></listener>
	</listeners>


	<test name="Login Test">
		<classes>
			<class name="com.google.testcases.LoginTest" />

		</classes>
	</test>

	<test name="Create Account Test">
		<classes>
			<class name="com.google.testcases.CreateAccountTest" />

		</classes>
	</test>



</suite>
